<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <data>
        <variable
            name="dataVenue"
            type="user.peiso.com.au.entity.InformationEntity.VenueUserInfo" />
    </data>

    <TextView
        android:id="@+id/iTextVenue"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:textSize="16sp"
        android:text="@{dataVenue.venueName}"
        android:background="#151C27"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:paddingStart="20dp"
        android:paddingEnd="10dp"
        android:gravity="center_vertical"
        android:drawablePadding="8dp"
        android:maxLines="1"
        android:ellipsize="end"
        tools:text="Pellegrino Pizza"
        android:layout_marginTop="1dp"
        app:drawableEndCompat="@mipmap/icon_arrow_right_x" />

</layout>